ionsEstadoActual,\n    styling: {\n      columns: 6,\n    },\n    rules: {\n      required: true,\n    },\n  },\n  {\n    name: 'categoria',\n    type: FieldType.options,\n    label: PRESTACIONES.fields.categoria,\n    options: async () => {\n      return (await get('api/categorias/')).data.results.map((c) => ({\n        value: c.id,\n        label: c.nombre,\n      }));\n    },\n    styling: {\n      columns: 6,\n    },\n    rules: {\n      required: true,\n    },\n  },\n  {\n    name: 'prestador',\n    type: FieldType.options,\n    label: PRESTACIONES.fields.prestador,\n    options: async () => {\n      return (await get('api/prestadores/')).data.results.map((p) => ({\n        value: p.id,\n        label: p.matricula,\n      }));\n    },\n    styling: {\n      columns: 6,\n    },\n    rules: {\n      required: true,\n    },\n  },\n  {\n    name: 'cantidad',\n    type: FieldType.int,\n    label: PRESTACIONES.fields.cantidad,\n\n    styling: {\n      columns: 6,\n    },\n    rules: {\n      required: true,\n    },\n  },\n  {\n    name: 'periodo',\n    type: FieldType.options,\n    options: NOMENCLADOR.optionsPeriodoTope,\n    label: PRESTACIONES.fields.periodo,\n    styling: {\n      columns: 6,\n    },\n    rules: {\n      required: true,\n    },\n  },\n  {\n    name: 'fechaPractica',\n    type: FieldType.date,\n    label: PRESTACIONES.fields.fechaPractica,\n    styling: {\n      columns: 6,\n    },\n    rules: {\n      required: true,\n    },\n  },\n];\n\nexport default configPrestaciones;\n"]},"metadata":{},"sourceType":"module"}