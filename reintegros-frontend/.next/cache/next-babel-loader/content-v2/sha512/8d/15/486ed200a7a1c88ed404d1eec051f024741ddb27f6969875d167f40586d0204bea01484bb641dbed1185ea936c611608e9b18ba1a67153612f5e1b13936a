{"ast":null,"code":"import { jsx as _jsx } from \"react/jsx-runtime\";\nimport React from 'react';\nimport Select from '@material-ui/core/Select';\nimport Input from '@material-ui/core/Input';\nimport MenuItem from '@material-ui/core/MenuItem';\nimport FormControl from '@material-ui/core/FormControl';\nexport default function SelectGroups({\n  grupos,\n  rowData,\n  onSetUserGroups\n}) {\n  const [groupsSelected, setGroupsSelected] = React.useState(rowData.groups.map(grupo => grupo.name));\n\n  const handleChange = event => {\n    setGroupsSelected(event.target.value);\n  };\n\n  const handleClose = () => {\n    onSetUserGroups(groupsSelected, rowData.id);\n  };\n\n  return /*#__PURE__*/_jsx(FormControl, {\n    children: /*#__PURE__*/_jsx(Select, {\n      labelId: \"select-grupos\",\n      id: `select-grupos-${rowData.id}`,\n      multiple: true,\n      value: groupsSelected,\n      onChange: handleChange,\n      input: /*#__PURE__*/_jsx(Input, {}),\n      onClose: () => handleClose(),\n      style: {\n        width: 180\n      },\n      children: grupos !== undefined && grupos.results.map(grupo => /*#__PURE__*/_jsx(MenuItem, {\n        value: grupo.name,\n        children: grupo.name\n      }, grupo.name))\n    })\n  });\n}","map":null,"metadata":{},"sourceType":"module"}